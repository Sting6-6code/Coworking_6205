<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<Pane xmlns="http://javafx.com/javafx/23.0.1"
      xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="application.LoginController"
      prefHeight="400.0" prefWidth="600.0">
   <children>
      <Label layoutX="111.0" layoutY="158.0" prefHeight="25.0" prefWidth="74.0" text="Username">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <TextField fx:id="usernameField" layoutX="220.0" layoutY="158.0" />

      <Label layoutX="117.0" layoutY="208.0" prefHeight="25.0" prefWidth="74.0" text="Password">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <TextField fx:id="passwordField" layoutX="220.0" layoutY="208.0" />

      <Button fx:id="loginButton" layoutX="220.0" layoutY="255.0"
              mnemonicParsing="false" text="Login"
              onAction="#login" />

      <Button fx:id="registerButton" layoutX="317.0" layoutY="255.0"
              mnemonicParsing="false" text="Register"
              onAction="#goToRegister" />
   </children>
</Pane>
